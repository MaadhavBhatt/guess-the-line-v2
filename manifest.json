{
  "manifest_version": 3,
  "name": "Guess The Line v2",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://www.desmos.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.desmos.com/calculator/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_idle"
    }
  ]
}